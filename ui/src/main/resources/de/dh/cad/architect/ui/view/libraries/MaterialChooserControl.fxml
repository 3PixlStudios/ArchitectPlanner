	<center>
		<SplitPane dividerPositions="0.7" BorderPane.alignment="CENTER">
			<items>
				<BorderPane fx:id="mTreeParentPane">
					<bottom>
					</bottom>
				</BorderPane>
				<BorderPane fx:id="mContentPane" minWidth="400.0" prefWidth="400.0" SplitPane.resizableWithParent="false">
					<padding>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</padding>
				</BorderPane>
			</items>
		</SplitPane>
	</center>
